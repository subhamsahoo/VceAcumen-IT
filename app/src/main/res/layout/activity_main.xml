<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="@drawable/back"
    >

  <ImageView
      android:layout_width="180dip"
      android:layout_height="180dip"
      android:src="@drawable/acumen1"
      android:layout_marginTop="150dip"
      android:layout_marginLeft="100dip"
      android:visibility="visible"
      />

    <ImageView
        android:layout_width="65dip"
        android:layout_height="65dip"
        android:src="@drawable/sponcers"
        android:layout_marginLeft="210dip"
        android:layout_marginTop="95dip"
        android:id="@+id/imageView6"
        android:visibility="visible"/>
    <ImageView
        android:layout_width="65dip"
        android:layout_height="65dip"
        android:src="@drawable/events"
        android:layout_marginTop="95dip"
        android:layout_marginLeft="65dip"
        android:id="@+id/events"
        android:visibility="visible"/>
    <ImageView
        android:layout_width="65dip"
        android:layout_height="65dip"
        android:src="@drawable/vasavi"
        android:layout_marginTop="220dip"
        android:id="@+id/vasavi"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="65dip"
        android:layout_height="65dip"
        android:src="@drawable/about"
        android:layout_marginTop="345dip"
        android:layout_marginLeft="75dip"
        android:id="@+id/about"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="65dip"
        android:layout_height="65dip"
        android:src="@drawable/contct"
        android:layout_marginLeft="200dip"
        android:layout_marginTop="336dip"
        android:id="@+id/imageView5"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="30dip"
        android:layout_height="30dip"
        android:src="@drawable/youtube"
        android:visibility="visible"
        android:id="@+id/youtube"/>

    <ImageView
        android:layout_width="30dip"
        android:layout_height="30dip"
        android:id="@+id/google"
        android:src="@drawable/dabbler"
        android:visibility="visible"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="30dip"
        android:layout_height="30dip"
        android:id="@+id/facebook"
        android:src="@drawable/facebook"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/google"
        android:layout_alignStart="@+id/google"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="30dip"
        android:layout_height="30dip"
        android:id="@+id/aboutapp"
        android:layout_alignBottom="@+id/google"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/aboutapp"
        android:visibility="visible"/>

</RelativeLayout>
