<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="@drawable/back"
    >

<ImageView
    android:layout_width="270dp"
    android:layout_height="270dp"
    android:src="@drawable/acumen1"
    android:layout_marginLeft="110dp"
    android:layout_marginTop="180dp"
    android:id="@+id/imageView7" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/events"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="70dp"
        android:id="@+id/events"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/sponcers"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="120dp"
        android:id="@+id/imageView6"/>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/vasavi"
        android:layout_marginTop="290dp"
        android:id="@+id/vasavi"/>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/about"
        android:layout_marginTop="460dp"
        android:layout_marginLeft="100dp"
        android:id="@+id/about"/>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:src="@drawable/contct"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="450dp"
        android:id="@+id/imageView5"/>

    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/facebook"
        android:src="@drawable/facebook"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView7"
        android:layout_toEndOf="@+id/imageView7" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/youtube"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/youtube"/>

    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/google"
        android:src="@drawable/dabbler"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/facebook"
        android:layout_alignStart="@+id/facebook" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/aboutapp"
        android:layout_alignBottom="@+id/google"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/aboutapp"/>
</RelativeLayout>
