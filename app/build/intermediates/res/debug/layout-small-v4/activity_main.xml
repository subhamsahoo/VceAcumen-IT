<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="@drawable/back"
    >

  <ImageView
      android:layout_width="150dip"
      android:layout_height="150dip"
      android:src="@drawable/acumen1"
      android:layout_marginTop="170dip"
      android:layout_marginLeft="115dip"
      android:visibility="visible"
      />

    <ImageView
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:src="@drawable/sponcers"
        android:layout_marginLeft="220dip"
        android:layout_marginTop="125dip"
        android:id="@+id/imageView6"
        android:visibility="visible"/>
    <ImageView
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:src="@drawable/events"
        android:layout_marginTop="115dip"
        android:layout_marginLeft="85dip"
        android:id="@+id/events"
        android:visibility="visible"/>
    <ImageView
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:src="@drawable/vasavi"
        android:layout_marginTop="220dip"
        android:layout_marginLeft="40dp"
        android:id="@+id/vasavi"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:src="@drawable/about"
        android:layout_marginTop="330dip"
        android:layout_marginLeft="97dip"
        android:id="@+id/about"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:src="@drawable/contct"
        android:layout_marginLeft="215dip"
        android:layout_marginTop="320dip"
        android:id="@+id/imageView5"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="25dip"
        android:layout_height="25dip"
        android:src="@drawable/youtube"
        android:visibility="visible"
        android:id="@+id/youtube"/>

    <ImageView
        android:layout_width="25dip"
        android:layout_height="25dip"
        android:id="@+id/google"
        android:src="@drawable/dabbler"
        android:visibility="visible"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="25dip"
        android:layout_height="25dip"
        android:id="@+id/facebook"
        android:src="@drawable/facebook"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/google"
        android:layout_alignStart="@+id/google"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="25dip"
        android:layout_height="25dip"
        android:id="@+id/aboutapp"
        android:layout_alignBottom="@+id/google"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/aboutapp"
        android:visibility="visible"/>

</RelativeLayout>
