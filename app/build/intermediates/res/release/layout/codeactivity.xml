<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="#000000"
    >


    <ImageView
        android:layout_width="400dp"
        android:layout_height="400dp"
        android:src="@drawable/acumen"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView3"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="70dip"
        android:layout_height="70dip"
        android:id="@+id/kodernan"
        android:src="@drawable/kodernan"
        android:layout_alignTop="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="70dip"
        android:layout_height="70dip"
        android:id="@+id/mosiac"
        android:layout_alignBottom="@+id/kodernan"
        android:layout_alignRight="@+id/imageView3"
        android:layout_alignEnd="@+id/imageView3"
        android:src="@drawable/mosiac"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="70dip"
        android:layout_height="70dip"
        android:id="@+id/cshoot"
        android:src="@drawable/cshoot"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:visibility="visible"/>

    <ImageView
        android:layout_width="70dip"
        android:layout_height="70dip"
        android:id="@+id/tictac"
        android:layout_alignBottom="@+id/cshoot"
        android:layout_alignRight="@+id/imageView3"
        android:layout_alignEnd="@+id/imageView3"
        android:src="@drawable/tic"
        android:visibility="visible"/>


